@model IEnumerable<ContosoSite.Models.Status>

@{
    ViewBag.Title = "Статусы выставок";
}

<h2>ВСЕ СТАТУСЫ ВЫСТАВОК</h2>


<table class="table">
    <tr>
        <th>
            Статус
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_status }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_status }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_status })
        </td>
    </tr>
}

</table>
